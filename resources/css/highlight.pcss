/* HighlightJS https://highlightjs.org/ */

/**
 * Rainglow 'blink' color theme.
 *
 * https://rainglow.io
 *
 * @author Dayle Rees
 */

.hljs {
    display: block;
    overflow-x: auto;
    background: #ffffff;
    color: #6a7e89;
}

.hljs-keyword,
.hljs-attr {
    color: #d4856a;
}

.hljs-function .hljs-keyword {
    color: #5298c4;
}

.hljs-name {
    color: #5298c4;
}

.hljs-function .hljs-title {
    color: #43b5b3;
}

.hljs-comment {
    color: #a6b4bc;
}

.hljs-doctag {
    color: #7a8f9b;
}

.hljs-string,
.hljs-symbol {
    color: #84c4ce;
}

.hljs-literal,
.hljs-number {
    color: #529ca8;
}
